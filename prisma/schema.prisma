generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(uuid())
  first_name    String
  last_name     String
  nick          String?
  email         String   @unique
  password_hash String
  created_at    DateTime @default(now())
}
